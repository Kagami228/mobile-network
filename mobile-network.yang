module subscribers {

  namespace "http://example.com/subscribers";
  prefix subscribers;

  import ietf-yang-types { prefix yang; }
	
  container discription {
    config true;
	list user{
		leaf number{
			type string;
			default "+380000000000";
			description "user phone number.mandatory filled";
			mandatory true;
		}
		
		leaf userName {
			type string;
			description "user name.";
		}
		list  incomingNumber{
			type number;
			//type userName;
			description "incoming number";
		}
		leaf state{
			type enumeration {
				
				enum  busy{
					value 1;
					description
					"busy";
				}
				enum  active{
					value 2;
					description
					"active";
				}
				enum  idle{
					value 3;
					description
					"idle";
				}
			}
			description "status. mandatory filled";
			mandatory true;
		}
	  }
	}
 }